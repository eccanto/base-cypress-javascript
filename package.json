{
  "name": "cypress-javascript-example",
  "version": "1.0.0",
  "description": "Cypress + Javascript",
  "scripts": {
    "cypress:open": "export $(grep -v '^#' .env | xargs) && cypress open",
    "pretest": "rm -rf cypress/{downloads,screenshots,videos}",
    "test": "export $(grep -v '^#' .env | xargs) && cypress run",
    "test:suitability": "export $(grep -v '^#' .env | xargs) && cypress run -e TAGS='@suitability'",
    "lint": "eslint . --ext .js,.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "cypress",
    "javascript",
    "BDD",
    "qa",
    "testing"
  ],
  "author": {
    "name": "Erik Ccanto",
    "email": "ccanto.erik@gmail.com",
    "url": "https://github.com/eccanto"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^10.7.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsonc": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "html-to-text": "^8.2.1",
    "mailslurp-client": "^15.13.3",
    "@badeball/cypress-cucumber-preprocessor": "^13.0.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/integration/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
